{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "mapping_basic",
  "title": "Conversion of mapping_basic",
  "description": "Test a mapping with all types",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "str": {
      "type": "string"
    },
    "int": {
      "type": "integer"
    },
    "float": {
      "type": "number"
    },
    "number": {
      "type": "number"
    },
    "text": {
      "oneOf": [
        {
          "type": "string"
        },
        {
          "type": "number"
        }
      ]
    },
    "bool": {
      "type": "boolean"
    },
    "date": {},
    "time": {},
    "timestamp": {},
    "seq": {
      "type": "array"
    },
    "map": {
      "type": "object",
      "additionalProperties": false
    },
    "scalar": {
      "oneOf": [
        {
          "type": "string"
        },
        {
          "type": "number"
        },
        {
          "type": "integer"
        },
        {
          "type": "boolean"
        }
      ]
    },
    "any": {}
  }
}