{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "additionalProperties": false,
    "properties": {
        "str": {
            "type": "string"
        },
        "int": {
            "type": "integer"
        },
        "float": {
            "type": "number"
        },
        "number": {
            "type": "number"
        },
        "text": {},
        "bool": {
            "type": "boolean"
        },
        "date": {},
        "time": {},
        "timestamp": {},
        "seq": {
            "type": "array"
        },
        "map": {
            "type": "object",
            "additionalProperties": false
        },
        "scalar": {
            "oneOf": [
                {
                    "type": "string"
                },
                {
                    "type": "number"
                },
                {
                    "type": "integer"
                },
                {
                    "type": "boolean"
                }
            ]
        },
        "any": {}
    }
}